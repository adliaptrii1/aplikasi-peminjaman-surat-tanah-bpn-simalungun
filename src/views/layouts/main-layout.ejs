<!-- Buatkan starter template bootstrap -->
<!DOCTYPE html>
<html>
  <head>
      <title><%= title %>  | ATR/BPN Simalungun</title>
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <!-- Gambar -->
      <link rel="icon" href="/img/bpn-logo.png" type="image/x-icon">
      <link
          rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
          integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
          crossorigin="anonymous"
          referrerpolicy="no-referrer"/>
      <link
          href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
          rel="stylesheet"
          integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM"
          crossorigin="anonymous"
      />
      <link rel="stylesheet" type="text/css" href="css/style.css">
      <link rel="stylesheet" type="text/css" href="css/style-auth.css">
      <link rel="stylesheet" type="text/css" href="css/style-form.css">
      <link rel="stylesheet" type="text/css" href="css/style-table.css">
      <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/jwt-decode/build/jwt-decode.min.js"></script>
    </head>

  <body>
    <script src="/js/script.js"></script>

        <%- include('nav') %>
        

        <% if (message) { %>
            <% if (!isDanger) { %>
                <div class="alert alert-success alert-dismissible fade show bg-success text-white border-0" role="alert">
                    <strong><%=message %></strong>
                    <button type="button" class="btn-close " data-bs-dismiss="alert" aria-label="Close" onclick="closeAlert()"></button>
                </div>
            <%} else {%>
                <div class="alert alert-danger alert-dismissible fade show bg-danger text-white border-0" role="alert">
                    <strong><%=message %></strong>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" onclick="closeAlert()"></button>
                </div>
            <% } %>
        <% } %>
      <!--end-->
      
      <!-- Disini tempat tergantung url yang dipanggil -->
      <%- body %>

      <!--end-->
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
      
    </body>

</html>